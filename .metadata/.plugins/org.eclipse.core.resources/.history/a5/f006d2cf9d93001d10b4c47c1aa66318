package com.StaticExample;

public class StaticMethod2 {

	int a=2;
	static int b=1;
	public void m1() {
		a++;
		b++;
		System.out.println(a);
		System.out.println(b);
	}
	
	public void m1(int c)
	{
		a++;
	    b=c;
	    System.out.println(a);
	    System.out.println(b);
		
	}
	
	public static void m2() {
		
		//a++;
		b++;
		//System.out.println(a);
		System.out.println(b);
		
	}
	
	public static void m2(int d) {
		b++;
		b=d;
		System.out.println(b);
		
	}
	
}
