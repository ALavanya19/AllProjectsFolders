package com.PatternExamples;

public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=34543,r,temp=0;
		if(n>0)
		{
			r=n%10;
			temp=(temp*10)+r;
			n=n/10;
		}
		
		if(n==temp)
		{
			System.out.println("matched");
		}
		else
		{
			System.out.println("Not matched");
		}
		
	}

}
