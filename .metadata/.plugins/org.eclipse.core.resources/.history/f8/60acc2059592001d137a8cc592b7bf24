package com.Inheritance;

public class InheritanceEx2 {

	int a=10;
	static int b=20;
	
	InheritanceEx2(){
		
		System.out.println("Ex2 Constructor Called");
	}
	
	void InheritanceEx1(){
		
		System.out.println("Ex1 Contructor called in ex2 class");
	}
	
	public String m1() {
		
		System.out.println("Normal Method");
		
		return "Normal Method using return type";
	}
	
    public int m1(int x,int y) {
		System.out.println(x+y);

		return x+y;
	}
	
	
	public static void m2() {
		
		System.out.println("Static method");
	}
	
}
