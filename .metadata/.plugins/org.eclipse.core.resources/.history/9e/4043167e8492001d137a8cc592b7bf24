package com.StaticExample;

public class StaticVariableColleageExample {

	String SName;
	static int rollno;
	static String collegeName;
	
	public void enroll(String Sname) {
		
		SName=Sname;
		rollno++;
	}
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		StaticVariableColleageExample s1=new StaticVariableColleageExample("Siva");
		 
	}

}
