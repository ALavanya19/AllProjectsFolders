		int size=20;
		
		for(int i=2;i<size;i++) {
		if(i==0 || i==1)
		{
			System.out.println(i+" not Prime number");
		}
		else
		{
			if(i%2==0)
				{
					System.out.println(i+" Not Prime number");
					
				}
				else if(i%1==0 && i%i==0) {
					
					System.out.println(i+" Prime Number");
				}
		}	}
		
	}
}
