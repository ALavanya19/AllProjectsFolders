package com.Inheritance;

public class B extends A {

	public void m4() {
		System.out.println("M4 called");
	}
	public void m5() {
		System.out.println("M5 called");
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
         B b=new B();
         b.m1();
         b.m2();
         b.m3();
         b.m4();
         b.m5();
	}

}
