package com.PatternExamples;

public class PrimeNum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	/*	int n=10;
		if(n==0|n==1)
		{
			System.out.println("not Prime Number");
		}
		else if(n%2==0)
		{
			System.out.println("not Prime number");
		}
		else if(n%n==0 && n%1==0)
