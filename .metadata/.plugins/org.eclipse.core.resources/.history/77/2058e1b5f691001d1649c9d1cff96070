		int size=10,n=3;
		if(n==0 || n==1)
		{
			System.out.println(n+" not Prime number");
		}
		else
		{
			for(int i=2;i<size;i++)
			{
				if(i%2==0)
				{
					System.out.println(i+" Not Prime number");
				}
				else if(i%1==0 && i%i==0) {
					
					System.out.println(i+" Prime Number");
				}
			}
		}
		
	}
}
