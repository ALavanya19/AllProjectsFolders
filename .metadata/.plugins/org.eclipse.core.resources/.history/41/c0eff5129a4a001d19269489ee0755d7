package com.ConditionalOperator;

public class IfelseExample {

	public static void main(String[] args) {
		
		int var1=150;
		float var2=105.5f;
		
		if(var1<var2)
		{
			System.out.println("Var1 varible is lesser than var2 variable");
		}
		else
		{
			System.out.println("Var1 is greather than var2");
		}
		
	}
}
