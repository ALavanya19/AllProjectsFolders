package com.PatternExamples;

public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=34543,r,sum=0,temp;
		temp=n;
		if(n>0)
		{
			r=n%10;
			sum=(sum*10)+r;
			n=n/10;
		}
		
		if(temp==sum)
		{
			System.out.println("matched");
		}
		else
		{
			System.out.println("Not matched");
		}
		
	}

}
