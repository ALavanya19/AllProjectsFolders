package com.ConditionalOperator;

public class IfelseExample {

	public static void main(String[] args) {
		
		int var1=150;
		float var2=105.5f;
		String str="Love";
		
		if(str>var2)
		{
			System.out.println("Var1 is greater than var2");
		}
		else
		{
			System.out.println("Var1 is lesser than var2");
		}
		
	}
}
